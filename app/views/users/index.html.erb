<div class="well linkr-links-container">
	<% @users.each do |user| %>
		<div class="row">
			<div class="col-xs-3">
				<%= user.created_at %>
			</div>
			<div class="col-xs-4">
				<%= link_to user.email, user %>
			</div>
			<div class="col-xs-4">
				<%= user.auth_token %>
			</div>
			<%= link_to "", user_path(user), :id => "delete_#{user.id}", method: :delete,
        data: {confirm: "Are you sure?"}, class: "glyphicon glyphicon-remove linkr-remove-link" %>
		</div>
	<% end %>
</div>





